{
    "name": "persist/sql-query",
    "type": "library",
    "description": "SQL Query builder and parser",
    "keywords": ["database", "db", "sql", "query", "query builder"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/jasny/persist/issues",
        "source": "https://github.com/jasny/persist"
    },
    "require": {
        "php": ">=7.4.0"
    },
    "require-dev": {
        "jasny/php-code-quality": "2.6.*"
    },
    "autoload": {
        "psr-4": {
            "Persist\\SQL\\Query\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Persist\\Tests\\SQL\\Query\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpstan analyse",
            "phpunit --testdox --colors=always",
            "phpcs -p src"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
